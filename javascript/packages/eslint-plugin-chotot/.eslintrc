{
  "root": true,
  "extends": "../eslint-config-chotot-base/index.js",
  "rules": {
    "chotot/no-unsafe-regex": 0,
    "consistent-return": 0,
    "default-case": 0,
    "import/no-dynamic-require": 0,
    "no-shadow": 0,
    "no-use-before-define": 0,
    "no-tabs": 0,
    "prefer-const": 0,
    "prefer-destructuring": 0,
    "strict": 0
  }
}
