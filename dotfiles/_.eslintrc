// Use this file as a starting point for your project's .eslintrc.
// Copy this file, and add rule overrides as needed.
// Remove import/resolver setting below if unused
{
  "extends": ["chotot"],
  "settings": {
    "import/resolver": "babel-plugin-root-import"
  },
  "env": {
    "browser": true
  },
  "rules": {
    "no-console": "warn"
  }
}
